// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Activity {
  id        Int      @id @default(autoincrement())
  type      String
  name      String?
  duration  Int?
  reps      Int?
  distance  Int?
  timestamp DateTime
}

model Vitals {
  id        Int      @id @default(autoincrement())
  weight    Int
  height    Int
  timestamp DateTime
}
